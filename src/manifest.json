{
  "manifest_version": 2,
  "name": "Netfix",
  "version": "0.1.0",
  "permissions": [
    "https://www.netflix.com/Kids*",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.netflix.com/Kids*"
      ],
      "js": [
        "vendor/jquery-3.1.1.min.js",
        "script.js"
      ],
      "run_at": "document_start"
    }
  ],
  "options_page": "options.html"
}
